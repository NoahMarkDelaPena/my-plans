{
  "expo": {
    "name": "my-plans",
    "slug": "my-plans",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.noahmark.myplans"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "my-plans",
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "Regular": ["../assets/fonts/Gilroy-Regular.ttf"],
          "Light": ["../assets/fonts/Gilroy-Light.ttf"],
          "Medium": ["../assets/fonts/Gilroy-Medium.ttf"],
          "SemiBold": ["../assets/fonts/Gilroy-SemiBold.ttf"],
          "Bold": ["../assets/fonts/Gilroy-Bold.ttf"],
          "ExtraBold": ["../assets/fonts/Gilroy-ExtraBold.ttf"],
          "Black": ["../assets/fonts/Gilroy-Black.ttf"]
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d366d36d-3a27-425f-b1ba-b4b1362547ca"
      }
    }
  }
}
